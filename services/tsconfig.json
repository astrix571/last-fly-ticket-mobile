{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "jsx": "react-native",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "allowJs": true,
      "resolveJsonModule": true,
      "noEmit": true,
      "isolatedModules": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["./*"]
      }
    }
  }
  